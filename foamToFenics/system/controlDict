FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// This file does not participate in the mesh conversion, but must be present
// to prevent any complaints from OpenFOAM.

application     simpleFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

writeControl    runTime;

endTime         1;

deltaT          1;

writeInterval   1;

writeFormat     binary;

writePrecision  10;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;


// ************************************************************************* //
